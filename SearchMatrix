import java.util.ArrayList;
public class Solution {
    static boolean searchMatrix(ArrayList<ArrayList<Integer>> mat, int target) {
          ArrayList<Integer> a;
        for(int i=0;i<mat.size();i++)
        {
             a =new ArrayList<>();
             a = mat.get(i);
            for(int j=0;j<a.size();j++)
            {
                if(a.get(j)==target)return true;

            }
        }
        return false;
    }
}
